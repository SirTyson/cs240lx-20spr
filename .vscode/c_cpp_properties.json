{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${CS140E_PATH}/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "c99",
            "intelliSenseMode": "gcc-x86",
            "compilerArgs": [
                "-Og",
                "-Wall",
                "-nostdlib",
                "-nostartfiles",
                "-ffreestanding",
                "-mcpu=arm1176jzf-s",
                "-mtune=arm1176jzf-s",
                "-ggdb",
                "-Wno-unused-function",
                "-Wno-unused-variable",
                "-Wno-pointer-sign "
            ]
        }
    ],
    "version": 4
}